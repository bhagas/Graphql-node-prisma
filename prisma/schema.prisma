generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model fd_activities {
  id           String      @id @db.VarChar(255)
  status       Int?
  priority     String?     @db.VarChar(255)
  createdAt    DateTime    @db.Timestamptz(6)
  updatedAt    DateTime    @db.Timestamptz(6)
  deleted      DateTime?   @db.Timestamptz(6)
  freshdesk_id String?     @db.VarChar(255)
  fd_tickets   fd_tickets? @relation(fields: [freshdesk_id], references: [id])
}

model fd_agents {
  id             String    @id @db.VarChar(255)
  available      String?   @db.VarChar(255)
  ticket_scope   Int?
  type           String?   @db.VarChar(255)
  active         String?   @db.VarChar(255)
  email          String?   @db.VarChar(255)
  name           String?   @db.VarChar(255)
  phone          String?   @db.VarChar(255)
  last_active_at String?   @db.VarChar(255)
  createdAt      DateTime  @db.Timestamptz(6)
  updatedAt      DateTime  @db.Timestamptz(6)
  deleted        DateTime? @db.Timestamptz(6)
}

model fd_escalations {
  id              String      @id @db.VarChar(255)
  escalation_type String?     @db.VarChar(255)
  createdAt       DateTime    @db.Timestamptz(6)
  updatedAt       DateTime    @db.Timestamptz(6)
  deleted         DateTime?   @db.Timestamptz(6)
  freshdesk_id    String?     @db.VarChar(255)
  fd_tickets      fd_tickets? @relation(fields: [freshdesk_id], references: [id])
}

model fd_group {
  id         String    @id @db.VarChar(255)
  group_id   String?   @db.VarChar(255)
  group_name String?   @db.VarChar(255)
  createdAt  DateTime  @db.Timestamptz(6)
  updatedAt  DateTime  @db.Timestamptz(6)
  deleted    DateTime? @db.Timestamptz(6)
}

model fd_priority {
  id          String    @id @db.VarChar(255)
  priority_id String?   @db.VarChar(255)
  name        String?   @db.VarChar(255)
  createdAt   DateTime  @db.Timestamptz(6)
  updatedAt   DateTime  @db.Timestamptz(6)
  deleted     DateTime? @db.Timestamptz(6)
}

model fd_status {
  id        String    @id @db.VarChar(255)
  status_id String?   @db.VarChar(255)
  name      String?   @db.VarChar(255)
  createdAt DateTime  @db.Timestamptz(6)
  updatedAt DateTime  @db.Timestamptz(6)
  deleted   DateTime? @db.Timestamptz(6)
}

model fd_tickets {
  id                      String           @id @db.VarChar(255)
  json                    Json?
  ticket_id               BigInt?          @unique
  cc_emails               Json?
  fwd_emails              Json?
  reply_cc_emails         Json?
  ticket_cc_emails        Json?
  tags                    Json?
  email_config_id         BigInt?
  group_id                BigInt?
  priority                Int?
  requester_id            BigInt?
  requester_name          String?
  requester_email         String?
  responder_id            BigInt?
  source                  Int?
  status                  Int?
  subject                 String?
  company_id              BigInt?
  type                    String?          @db.VarChar(255)
  to_emails               String?
  product_id              BigInt?
  fr_escalated            Boolean?
  spam                    Boolean?
  urgent                  Boolean?
  is_escalated            Boolean?
  fd_created_at           DateTime?        @db.Timestamptz(6)
  fd_updated_at           DateTime?        @db.Timestamptz(6)
  due_by                  DateTime?        @db.Timestamptz(6)
  fr_due_by               DateTime?        @db.Timestamptz(6)
  nr_due_by               DateTime?        @db.Timestamptz(6)
  nr_escalated            Boolean?
  json_custom_field       Json?
  cf_best_number_to_reach String?          @db.VarChar(255)
  cf_best_number_note     String?          @db.VarChar(255)
  cf_quotewekrs           String?          @db.VarChar(255)
  cf_qbsalesorder         String?          @db.VarChar(255)
  cf_qbinv                String?          @db.VarChar(255)
  cf_totalhours           String?          @db.VarChar(255)
  createdAt               DateTime         @db.Timestamptz(6)
  updatedAt               DateTime         @db.Timestamptz(6)
  deleted                 DateTime?        @db.Timestamptz(6)
  fd_activities           fd_activities[]
  fd_escalations          fd_escalations[]
}

model role_pool {
  id        String    @id @db.VarChar(255)
  createdAt DateTime  @db.Timestamptz(6)
  updatedAt DateTime  @db.Timestamptz(6)
  deletedAt DateTime? @db.Timestamptz(6)
  userId    String?   @db.VarChar(255)
  roleId    String?   @db.VarChar(255)
  roles     roles?    @relation(fields: [roleId], references: [id])
  users     users?    @relation(fields: [userId], references: [id])
}

model roles {
  id        String      @id @db.VarChar(255)
  role_name String      @db.VarChar(255)
  code      String?     @db.VarChar(255)
  createdAt DateTime    @db.Timestamptz(6)
  updatedAt DateTime    @db.Timestamptz(6)
  deleted   DateTime?   @db.Timestamptz(6)
  role_pool role_pool[]
}

model users {
  id                String             @id @db.VarChar(255)
  name              String?            @db.VarChar(255)
  email             String?            @db.VarChar(255)
  password          String?            @db.VarChar(255)
  confirmation_code String?            @db.VarChar(255)
  status            enum_users_status? @default(pending)
  createdAt         DateTime           @db.Timestamptz(6)
  updatedAt         DateTime           @db.Timestamptz(6)
  deleted           DateTime?          @db.Timestamptz(6)
  role_pool         role_pool[]
}

enum enum_users_status {
  pending
  active
  inactive
}
